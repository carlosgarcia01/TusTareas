{
  "name": "tustareas",
  "version": "0.0.0",
  "description": "api rest de tus tareas",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "start": "node src/index.js",
    "test": "cross-env NODE_ENV=test nyc mocha --timeout=15000 --reporter spec --exit",
    "generate-secret": "node -e console.log(require('crypto').randomBytes(256).toString('base64'));",
    "lint": "eslint . && prettier --check \"**/*.{json,md,html,yaml,css}\"",
    "lint:fix": "eslint . --fix && prettier --write \"**/*.{json,md,html,yaml,css}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,html,yaml,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@kazaar/express-error-handler": "^2.4.2",
    "body-parser": "^1.18.3",
    "celebrate": "^10.0.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-promise-router": "^3.0.3",
    "helmet": "^3.15.0",
    "http-errors": "^1.7.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mongoose": "^5.7.8",
    "morgan": "^1.9.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-chai-expect": "^2.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "mocha": "^6.2.2",
    "nodemon": "^1.18.9",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": ">=10.16"
  }
}
